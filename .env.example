# Railway Environment Variables
# These will be set automatically by Railway

# Port (Railway sets this automatically)
PORT=3000

# Database (Railway PostgreSQL - will be auto-configured)
DATABASE_URL=${{Postgres.DATABASE_URL}}

# Node Environment
NODE_ENV=production

# CORS Origin (will be your Railway app URL)
CORS_ORIGIN=https://${{RAILWAY_PUBLIC_DOMAIN}}

# Rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Code execution limits
CODE_EXECUTION_TIMEOUT=5000
MAX_CODE_SIZE=1048576

# Session security
SESSION_SECRET=${{RAILWAY_ENVIRONMENT_NAME}}_secret_key

# Admin password (set this in Railway dashboard)
ADMIN_PASSWORD=your_secure_admin_password_here
